            <div class="header-middle sticky-header">
                <div class="container">
                    <div class="header-left">
                        <button class="mobile-menu-toggler">
                            <span class="sr-only">Toggle mobile menu</span>
                            <i class="icon-bars"></i>
                        </button>

                        <a href="index.html" class="logo">
                            <img src="/Public/user/images/logo.jpg" alt="Molla Logo" width="105" height="25">
                        </a>

                        <nav class="main-nav">
                            <ul class="menu sf-arrow">
                                <% categories.forEach((data)=>{ %>
                
                                <li>
                                    <a href="/product?catId=<%=data._id%>" class="sf-with-ul"><%=data.name%></a>

                                    <div class="megamenu megamenu-sm">
                                        <div class="row no-gutters">
                                            <div class="col-md-6">
                                                <div class="menu-col">
                                                    <div class="menu-title"><%=data.name%> brands</div><!-- End .menu-title -->
                                                    <ul>
                                                        <% brands.forEach((brand)=>{%>
                                                        <li><a href="/product?catId=<%=data._id%>&brand=<%=brand%>"><%=brand%></a></li>
                                                        <% }) %>
                                                    </ul>
                                                </div><!-- End .menu-col -->
                                            </div><!-- End .col-md-6 -->

                                            <div class="col-md-6">
                                                <div class="banner banner-overlay">
                                                    <a href="category.html">
                                                        <img src="/Public/user/images/categories/<%=data.image%>" alt="Banner">

                                                        <div class="banner-content banner-content-bottom">
                                                            <div class="banner-title text-white">New Trends<br><span><strong>spring 2022</strong></span></div>End
                                                        </div>
                                                        <!-- End .banner-content -->
                                                    </a>
                                                </div>
                                                <!-- End .banner -->
                                            </div><!-- End .col-md-6 -->
                                        </div><!-- End .row -->
                                    </div><!-- End .megamenu megamenu-sm -->
                                </li>
                                <% }) %>

                                

                                
                               
                       
                          
